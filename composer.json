{
    "name": "kanalumaddela/k-load-v2",
    "description": "A loading screen for Garry's Mod",
    "type": "project",
    "config": {
        "platform": {
            "php": "8.5"
        },
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/kanalumaddela/SimpleCache"
        }
    ],
    "require": {
        "php": ">=8.4",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "ext-xml": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "ext-pdo": "*",
        "j0sh0nat0r/simple-cache": "dev-master",
        "xpaw/steamid": "dev-master",
        "kanalumaddela/steam-login": "dev-master",
        "twig/twig": "^3.22",
        "symfony/http-foundation": "^8.0",
        "illuminate/support": "^12.40",
        "illuminate/database": "^12.40",
        "phroute/phroute": "^2.2",
        "league/container": "^5.1",
        "opis/closure": "^4.4",
        "adbario/php-dot-notation": "^3.3",
        "illuminate/pagination": "^12.40",
        "matthiasmullie/minify": "^1.3",
        "symfony/mime": "^8.0",
        "sentry/sentry": "^4.19"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "filp/whoops": "^2.18",
        "symfony/var-dumper": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "KLoad\\": "inc/classes/KLoad",
            "KLoad\\Plugins\\": "plugins"
        },
        "files": [
            "inc/helpers.php",
            "inc/error.php",
            "inc/init.php"
        ]
    }
}
