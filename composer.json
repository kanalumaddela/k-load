{
    "name": "kanalumaddela/k-load-v2",
    "description": "A customizable loading screen framework designed for Garry's Mod",
    "type": "project",
    "config": {
        "platform": {
            "php": "8.4"
        },
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require": {
        "php": ">=8.4",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "ext-xml": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "ext-pdo": "*",
        "kanalumaddela/steam-login": "dev-master",
        "flightphp/core": "^3.17"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    }
}
