{
    "name": "kanalumaddela/k-load-v2",
    "description": "A loading screen for Garry's Mod",
    "type": "project",
    "config": {
        "platform": {
            "php": "8.0"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/kanalumaddela/SimpleCache"
        }
    ],
    "require": {
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "ext-xml": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "ext-pdo": "*",
        "j0sh0nat0r/simple-cache": "dev-master",
        "twig/twig": "^3.3",
        "kanalumaddela/steam-login": "dev-master",
        "sentry/sdk": "^3.1",
        "symfony/http-foundation": "^5.2",
        "illuminate/support": "^8.41",
        "illuminate/database": "^8.41",
        "xpaw/steamid": "dev-master",
        "phroute/phroute": "^2.1",
        "league/container": "^3.3",
        "opis/closure": "^3.6",
        "adbario/php-dot-notation": "^2.2",
        "illuminate/pagination": "^8.41",
        "matthiasmullie/minify": "^1.3",
        "symfony/mime": "^5.4"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "filp/whoops": "^2.7",
        "symfony/var-dumper": "^4.4",
        "laravel/pint": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "KLoad\\": "inc/classes/KLoad",
            "KLoad\\Plugins\\": "plugins"
        },
        "files": [
            "inc/helpers.php",
            "inc/error.php",
            "inc/init.php"
        ]
    }
}
