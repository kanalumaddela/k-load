{
    "preset": "psr12",
    "rules": {
        "global_namespace_import": {
            "import_functions": true
        },
        "native_function_invocation": {
            "exclude": [
                "redirect",
                "defineConstant",
                "get_vars",
                "is_https",
                "var_export_fixed"
            ],
            "include": [
                "@all"
            ],
            "scope": "namespaced"
        }
    },
    "exclude": [],
    "notPath": []
}